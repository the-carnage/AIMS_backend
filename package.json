{
  "name": "aims-backend",
  "version": "1.0.0",
  "description": "AIMS Backend - Atlas Interceptor Mission Simulator API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "postbuild": "node -e \"console.log('Backend built')\"",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "swagger-jsdoc -d swagger.config.js src/routes/*.ts -o docs/swagger.json"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.4",
    "axios": "^1.6.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "typescript": "^5.2.2",
    "tsx": "^3.14.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "keywords": [
    "space",
    "simulation",
    "orbital-mechanics",
    "nasa",
    "3I/ATLAS",
    "hackathon"
  ],
  "author": "AIMS Development Team",
  "license": "MIT"
}